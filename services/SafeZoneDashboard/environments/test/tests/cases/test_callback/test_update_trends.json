[
    {
        "test_describes": "test callback to get city trend data",
        "api_path": "cases/city",
        "mock_cities": [
            "台北市",
            "高雄市"
        ],
        "params_template": {
            "now": "<current_date>",
            "city": "<mock_city>",
            "interval": "14"
        },
        "expected": {
            "response_template": {
                "data": "<data_template.mock_city.now>",
                "message": "Data returned successfully",
                "detail": "Data returned successfully for dates <start_date> ~ <end_date>.",
                "success": true
            },
            "data_template": {
                "<mock_city>": {
                    "<now>": {
                        "start_date": "<now - interval>",
                        "end_date": "<now>",
                        "city": "<mock_city>",
                        "aggregated_cases": "<random_or_fixed_value>"
                    }
                }
            }
        }
    }
]